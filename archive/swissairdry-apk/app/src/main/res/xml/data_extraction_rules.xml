<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude specific shared preferences that contain sensitive information -->
        <exclude
            domain="sharedpref"
            path="auth_preferences.xml" />
        <exclude
            domain="sharedpref"
            path="com.swissairdry.mobile.auth.xml" />
        <exclude
            domain="database"
            path="auth_database.db" />
    </cloud-backup>
    <device-transfer>
        <exclude
            domain="sharedpref"
            path="auth_preferences.xml" />
        <exclude
            domain="sharedpref"
            path="com.swissairdry.mobile.auth.xml" />
        <exclude
            domain="database"
            path="auth_database.db" />
    </device-transfer>
</data-extraction-rules>